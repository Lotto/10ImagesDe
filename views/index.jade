//- index.jade
extends ./layout.jade

block title
  title= title

block content
  h1.generated-title= title
  h1.generated-title= subtitle
    span.subject
  .ten-images 
    each val, index in images
      .one-image
        .index= index + 1
        img.lazy(data-src=val.link)

  script(type='text/javascript').
    $(function() {
        $('.lazy').Lazy();
    });